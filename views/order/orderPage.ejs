<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Order List</title>
  </head>
  <body>
    <h1>Order List</h1>
    <ul>
      <% orders.forEach(order => { %>
      <li>
        <a href="/orders/<%= order.orderId %>"
          >Order ID: <%= order.orderId %></a
        >
        - <%= order.product %> - <%= order.price %> USD
      </li>
      <% }) %>
    </ul>
    <a href="/">Main Page</a>
  </body>
</html>
